{
    "name": "GameNightPlanner",
    "version": "1.0.0",
    "size": {
      "width": 6,
      "height": 6
    },
    "mapping": [
      {
        "alias": "boardgames",
        "dataSetId": "[YOUR_DATASET_ID]",
        "fields": []
      }
    ],
    "collections": [
      {
        "name": "ownership",
        "schema": {
          "columns": [
            {
              "name": "boardGameId",
              "type": "LONG"
            },
            {
              "name": "owned",
              "type": "STRING"
            }
          ]
        },
        "syncEnabled": true
      }
    ],
    "fileName": "manifest.json",
    "id": "[YOUR_APP_DESIGN_ID]",
    "proxyId": "[YOUR_PROXY_ID]"
  }
  